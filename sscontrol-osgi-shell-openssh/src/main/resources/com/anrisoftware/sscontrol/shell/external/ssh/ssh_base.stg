sshDebug(args) ::= <%
<args.debugLevel>
%>

sshArgs(args) ::= <%
<args.sshArgs;separator=" ">
%>

sshOptions(args, options) ::= <%
<first(options):{it|-o }><options;separator=" -o ">
%>

sshKeyOption(args) ::= <%
-i <args.sshKey>
%>

sshChdir(chdir) ::= <<
<if(chdir)>cd "<chdir>"<endif>
>>

sshEnv(env) ::= <<
<env.keys:{k | export <k>=<env.(k)>};separator="\n">
>>
